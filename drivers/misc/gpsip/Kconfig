#
# CGX GPS subsystem configuration
#
menu "GPS modules"
config CGX
	bool "Sprd gps"
	default n
	help
	  GPS can locate you position

config CGX_DRIVER
	tristate "Sprd gps driver"
	depends on CGX
	help
	  GPS can locate you position

config CGX_VIRTUALCOM
	tristate "Sprd gps virtual com"
	depends on CGX
	help
	  GPS can locate you position
endmenu
